pathArray = currentURL.split("/");
if (pathArray[1] == "es") { 
document.write('<link rel="canonical" href="http://www.clubpenguin.com/es/membership/choose_plan.php" /> ');  // i.e. an Spanish URL
} else if (pathArray[1] == "pt") { 
document.write('');  // i.e. an Portuguese URL
} else if (pathArray[1] == "fr") { 
document.write('');  // i.e. an French URL
} else { 
document.write('<link rel="canonical" href="http://www.clubpenguin.com/es/membership/choose_plan.php" />');  // i.e. an English URL
} 
</script>
